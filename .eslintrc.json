{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-alert": "off",
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "all",
        "semi": true,
        "printWidth": 80,
        "tabWidth": 2,
        "endOfLine": "auto"
      }
    ]
  }
}
